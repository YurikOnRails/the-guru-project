# Ignore bundler config
/.bundle

# Ignore Gems
/vendor/bundle
*.gem

# Ignore log files and temp files.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore database files.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore secrets.
/config/master.key
/config/credentials.yml.enc

# Ignore node modules and Yarn integrity file.
/node_modules
/yarn-error.log
/package-lock.json

# Ignore compiled assets.
/public/assets
/public/packs
/public/packs-test
/public/uploads

# Ignore coverage report generated by simplecov.
/coverage/

# Ignore IDE files
.idea/
*.iml
*.swp

# Ignore system files
*.bak
*.tmp

# Ignore byebug command history
.byebug_history

# Ignore Linux-specific system files
*~
*.lock
*.pid
*.log
