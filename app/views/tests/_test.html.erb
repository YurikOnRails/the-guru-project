<tr>
  <td><%= test.title %></td>
  <td><span class="badge bg-<%= test.level > 2 ? "danger" : (test.level > 0 ? "warning" : "success") %>"><%= test.level %></span></td>
  <td><%= test.category.name %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to t(".show"), test_path(test), class: "btn btn-sm btn-outline-primary" %></td>
  <td><%= button_to t(".start"), start_test_path(test), class: "btn btn-sm btn-success", method: :post %></td>
</tr>
